# Database Configuration
# For Docker Compose deployment, DATABASE_URL is auto-constructed using POSTGRES_PASSWORD
POSTGRES_PASSWORD=your_secure_password_here

# Access Codes (three-tier access system)
# Uploader: Can only access the upload page
UPLOADER_CODE=your_uploader_code_here

# Browser: Can only access the market browser
BROWSER_CODE=your_browser_code_here

# Admin: Full access to upload, market, statistics, changelog, and settings
ADMIN_CODE=your_admin_code_here

# OpenAI API (optional - for OpenAI models)
OPENAI_API_KEY=

# Anthropic API (optional - for Claude models)
ANTHROPIC_API_KEY=

# Google AI API (optional - for Gemini models)
GOOGLE_API_KEY=
