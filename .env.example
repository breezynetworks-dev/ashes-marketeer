# Database Configuration
# For Docker Compose deployment, DATABASE_URL is auto-constructed using POSTGRES_PASSWORD
POSTGRES_PASSWORD=your_secure_password_here

# Access Code (shared password for app access)
ACCESS_CODE=your_access_code_here

# Admin Code (needed for settings page access)
ADMIN_CODE=your_admin_code_here

# OpenAI API (optional - for OpenAI models)
OPENAI_API_KEY=

# Anthropic API (optional - for Claude models)
ANTHROPIC_API_KEY=

# Google AI API (optional - for Gemini models)
GOOGLE_API_KEY=
